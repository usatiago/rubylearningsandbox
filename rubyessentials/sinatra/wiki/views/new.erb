<h1>Add new page</h1>

<div>
    <form method="post" action="/create">
        <fieldset>
            <label for="title">Title</label>
            <input type="text" name="title">
        </fieldset>
        <fieldset>
            <label for="content">Page Content's</label>
            <textarea rows="10" columns="200" name="content"></textarea>
        </fieldset>
        <input type="submit">
    </form>
</div>
